{
  "name": "npm-reinstall",
  "version": "1.2.1",
  "description": "Just reinstall npm package.",
  "bin": {
    "reinstall": "./dist/bin.js",
    "rin": "./dist/bin.js"
  },
  "preferGlobal": true,
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "pretest": "npm run build && ts-node ./script/prepare-test.ts",
    "test": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/npm-reinstall.git"
  },
  "keywords": [
    "install",
    "reinstall",
    "package",
    "module"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/npm-reinstall/issues"
  },
  "homepage": "https://github.com/gluons/npm-reinstall",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.3.3",
    "@gluons/prettier-config": "^2.0.0",
    "@types/ansi-escapes": "^3.0.0",
    "@types/chalk-animation": "^1.6.0",
    "@types/cross-spawn": "^6.0.0",
    "@types/del": "^3.0.1",
    "@types/has-yarn": "^1.0.0",
    "@types/jest": "^24.0.6",
    "@types/lodash.once": "^4.1.5",
    "@types/node": "6",
    "@types/ora": "^3.0.0",
    "@types/update-notifier": "^2.5.0",
    "@types/yargs": "^12.0.9",
    "ansi-escapes": "^3.2.0",
    "babel-jest": "^24.1.0",
    "chalk-animation": "^1.6.0",
    "cross-env": "^5.2.0",
    "jest": "^24.1.0",
    "lodash.once": "^4.1.1",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.2",
    "tsconfig-gluons": "^1.0.1",
    "tslint": "^5.13.0",
    "tslint-config-gluons": "^2.1.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "@danielbayerlein/caniuse-yarn": "^1.1.0",
    "chalk": "^2.4.2",
    "cross-spawn": "^6.0.5",
    "del": "^3.0.0",
    "has-yarn": "^1.0.0",
    "ora": "^3.1.0",
    "update-notifier": "^2.5.0",
    "yargs": "^13.2.1"
  },
  "resolutions": {
    "deep-extend": "^0.6.0"
  }
}

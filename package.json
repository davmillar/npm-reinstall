{
  "name": "npm-reinstall",
  "version": "1.2.1",
  "description": "Just reinstall npm package.",
  "bin": {
    "reinstall": "./dist/bin.js",
    "rin": "./dist/bin.js"
  },
  "preferGlobal": true,
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "preparetest": "ts-node ./script/prepare-test.ts",
    "pretest": "run-p preparetest build",
    "test": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"allowJs\\\":true} mocha -r ts-node/register ./test/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/npm-reinstall.git"
  },
  "keywords": [
    "install",
    "reinstall",
    "package",
    "module"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/npm-reinstall/issues"
  },
  "homepage": "https://github.com/gluons/npm-reinstall",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/del": "^3.0.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.4",
    "@types/ora": "^3.0.0",
    "@types/update-notifier": "^2.5.0",
    "@types/yargs": "^12.0.9",
    "chai": "4.2.0",
    "chalk-animation": "^1.6.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "eslint-config-gluons": "^4.1.0",
    "lodash.once": "^4.1.1",
    "mocha": "6.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-gluons": "^2.1.0",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@danielbayerlein/caniuse-yarn": "^1.1.0",
    "chalk": "^2.4.2",
    "cross-spawn": "^6.0.5",
    "del": "^3.0.0",
    "has-yarn": "^1.0.0",
    "ora": "^3.1.0",
    "update-notifier": "^2.5.0",
    "yargs": "^13.2.1"
  }
}

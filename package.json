{
  "name": "npm-reinstall",
  "version": "1.1.1",
  "description": "Just reinstall npm package.",
  "bin": {
    "reinstall": "./reinstall.js",
    "rin": "./reinstall.js"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "scripts": {
    "precompile": "del-cli ./binary/",
    "compile": "pkg -t node6-win-x86,node6-win-x64,node6-linux-x86,node6-linux-x64 --out-dir ./binary/ .",
    "postcompile": "renamer -f 'npm-' './binary/*'",
    "build-docker": "./script/build-docker.sh",
    "test:local:node4": "docker run --rm npm-reinstall:node4",
    "test:local:node6": "docker run --rm npm-reinstall:node6",
    "test:local:node7": "docker run --rm npm-reinstall:node7",
    "test:local": "run-s test:local:*",
    "pretest": "node ./script/prepare-test.js",
    "test": "mocha",
    "posttest": "node ./script/clean-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/npm-reinstall.git"
  },
  "keywords": [
    "install",
    "reinstall",
    "package",
    "module"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/npm-reinstall/issues"
  },
  "homepage": "https://github.com/gluons/npm-reinstall",
  "devDependencies": {
    "chai": "*",
    "del-cli": "^1.0.0",
    "eslint": "^3.19.0",
    "eslint-config-gluons": "^2.0.1",
    "mocha": "*",
    "npm-run-all": "^4.0.2",
    "pkg": "^3.0.4",
    "renamer": "^0.6.1"
  },
  "dependencies": {
    "@danielbayerlein/caniuse-yarn": "^1.1.0",
    "arrvl": "^1.0.0",
    "chalk": "^1.1.3",
    "cross-spawn": "^5.1.0",
    "del": "^2.2.2",
    "has-yarn": "^1.0.0",
    "nvl": "^1.0.1",
    "ora": "^1.2.0",
    "update-notifier": "^2.1.0",
    "yargs": "^8.0.1"
  }
}
